<%= form_for @song do |f| %>
  <div class="form-group">
    <%= f.label :title %>
    <%= f.text_field :title %>
  </div>
  <div class="form-group">
    <%= f.label :artist %>
    <%= f.text_field :artist %>
  </div>
  <div class="form-group">
    <%= f.label :path %>
    <%= f.text_field :path %>
  </div>
  <div class="form-group">
    <%= f.submit "Create" %>
  </div>
<% end %>
